    <script>
		$(document).ready(function() {
		    $("#abre-menu").click(function() {
		        $("body").toggleClass("tp-menu-compacto");
		        return false;
		    });
		
		    $(".tp-tem-submenu").click(function() {
		        var current = $(this);
		        $(this).closest("ul").children("li").each(function() {
		            $(this).children("a.tp-menu-item-seta-para-cima").each(function() {
		                if (!$(this).is(current)) {
		                    $(this).removeClass("tp-menu-item-seta-para-cima");
		                }
		            });
		        });
		
		        $(this).toggleClass("tp-menu-item-seta-para-cima");
		
		        var submenu = $("ul", $(this).parent());
		
		        if (submenu.hasClass("tp-submenu-aberto")) {
		            submenu.removeClass("tp-submenu-aberto");
		        } else {
		            $("ul.tp-submenu-aberto").removeClass("tp-submenu-aberto");
		            submenu.addClass("tp-submenu-aberto");
		        }
		
		        return false;
		    });
		
		    $("#abre-busca").click(function() {
		        $("body").toggleClass("tp-busca-compacto");
		        return false;
		    });
		
		    $("#fecha-janela").click(function() {
		        $("body").removeClass("tp-menu-compacto");
		        $("body").removeClass("tp-busca-compacto");
		        return false;
		    });
		
