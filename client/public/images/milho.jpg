			// Submit do formulario de busca geral
		    $("#form-busca-geral").submit(function() {
		        var palavrasChave = $("#palavrasChave", this).val();
		        if (palavrasChave !== "") {
		            var action = $(this).attr("action");
		            palavrasChave = palavrasChave.replace(/[:]/g, "")
		                .replace(/\(/g, "%28")
		                .replace(/\)/g, "%29")
		                .replace(/\//g, "%2F");
		            action += "busca/" + encodeURIComponent(palavrasChave);
		            $(this).attr("action", action);
		            return true;
		        } else {
		            return false;
		        }
		    });
		});
    </script>

